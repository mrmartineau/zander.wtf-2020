{
  "version": 2,
  "alias": "zander- wtf-2020",
  "routes": [
    {
      "src": "/atom.xml",
      "headers": {
        "x-zander": "wtf"
      },
      "dest": "/scripts/build-rss.js"
    },
    {
      "src": "/public/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      },
      "dest": "/$1"
    }
  ],
  "scope": "zander"
}
